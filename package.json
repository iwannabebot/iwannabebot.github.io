{
  "name": "iwannabebot.github.io",
  "version": "0.0.0",
  "license": "MIT",
  "main": "build.js",
  "scripts": {
    "postinstall": "npm i -g rimraf nodemon gh-pages && npm rebuild node-sass",
    "dev": "concurrently \"nodemon\" \"live-server dist\"",
    "build": "rimraf dist && node build.js --output='dist' --view='robo.txt;CNAME;index.html;assets;blogs;tools' --style='style' --script='script'",
    "deploy": "gh-pages -d dist"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^4.1.2",
    "gh-pages": "^1.1.0",
    "live-server": "^1.2.1",
    "markdown-it": "^10.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0"
  }
}
